      "email": "mikestillman1@gmail.com"
    }
  ],
  "dependencies": {
    "async": "~0.9.0",
    "chai": "^2.2.0",
    "cookies": "~0.5.0",
    "express": "^4.10.8",
    "express-winston": "~0.2.9",
    "forever": "^0.14.1",
    "serve-favicon": "^2.1.7",
    "serve-static": "^1.7.2",
    "socket.io": "^1.2.1",
    "socket.io-cookie": "~0.0.1",
    "socketio-file-upload": "^0.2.7",
    "ssh2": "~0.4.6",
    "winston": "^0.8.3"
  },
  "scripts": {
    "start": "node src/lib/Macaulay2LocalServer.js",
    "sudo_docker": "node src/lib/Macaulay2SudoDocker.js",
    "ssh_docker": "node src/lib/Macaulay2SshDocker.js",
    "test": "mocha src/tests/ --reporter spec --timeout 6000",
    "stop": "kill `ps ax | grep Server | grep -v monitor | grep node | awk '{print $1}'`",
    "forever": "./node_modules/forever/bin/forever start -l /home/webm2.logs/forever.log -o /home/webm2.logs/out.log -e /home/webm2.logs/err.log --append src/lib/Macaulay2SshDocker.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:fhinkel/tryM2.git"
  },
  "devDependencies": {
    "jsdom": "^4.3.0",
    "mocha": "^2.1.0",
    "sinon": "^1.14.1",
    "supertest": "^0.15.0"
  }
}

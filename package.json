{
  "name": "InteractiveShell",
  "version": "1.1.0",
  "description": "Interactive shell, allows to use Macaulay2 or Singular in a browser",
  "contributors": [
    {
      "name": "Franziska Hinkelmann",
      "email": "franziska.hinkelmann@gmail.com"
